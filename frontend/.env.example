# Environment Configuration
REACT_APP_ENV=development

# API Configuration
REACT_APP_API_URL=http://localhost:3001
# For production, use your actual API Gateway URL:
# REACT_APP_API_URL=https://your-api-id.execute-api.eu-west-2.amazonaws.com/dev

# Feature Flags
REACT_APP_ENABLE_MOCK_DATA=false
REACT_APP_ENABLE_DEBUG=true

# External Services (Optional)
REACT_APP_SENTRY_DSN=
REACT_APP_GOOGLE_ANALYTICS_ID=

# NHS Integration (Future)
REACT_APP_NHS_LOGIN_ENABLED=false
REACT_APP_NHS_LOGIN_CLIENT_ID=

# Development Settings
REACT_APP_LOG_LEVEL=info
REACT_APP_API_TIMEOUT=30000
